/* pages/tool/ascii/ascii.wxss */
.title {
    font: bold 55rpx sans-serif;
    text-align: center;
    margin: 150rpx auto 65rpx auto;
}

.search {
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
    flex-wrap: wrap;
    height: 900rpx;
}

.word {
    border: 1rpx solid black;
    height: 50rpx;
    width: 65rpx;
    margin: 5rpx;
}

.result {
    text-align: center;
    margin: auto;
    display: flex;
    flex-direction: column;
    align-items: left;
}

.char {
    font: bold 35rpx sans-serif;
}

.ten {}

.two {}

